  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "yarn clean && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "prepare": "yarn build",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "yarn lint --fix",
